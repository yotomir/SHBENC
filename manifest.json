{
  "manifest_version": 3,
  "name": "Hidden Scratch Blocks Official",
  "version": "2.1",
  "content_scripts": [
    {
      "matches": ["*://scratch.mit.edu/projects/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["*://scratch.mit.edu/*"]
    }
  ]
}
